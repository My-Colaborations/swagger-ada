with "util";
with "util_http";
with "util_config";
with "security";

project Swagger is
   Version := "@SWAGGER_VERSION@";

   for Library_Name use "ada_swagger";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/swagger");
   for Library_ALI_Dir use Util_Config.Alidir & "/swagger/";
   for Externally_Built use "true";
end Swagger;
